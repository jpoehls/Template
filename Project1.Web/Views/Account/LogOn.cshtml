@using Project1.Web.Controllers
@using Project1.Web.Models

@{
    ViewBag.Title = "Log On";
}

<h2>
    @(ViewBag.Title)</h2>
<p>
    Please enter your user name and password. @Html.ActionLink("Register", "Register")
    if you don't have an account.
</p>
@using (var form = Html.FormForAction((AccountController ctlr, LogOnModel model, string r) => ctlr.LogOn(model, r)))
{
    form.Render();
}
<p>@Html.ActionLink("Forgotten password?", "RequestPasswordReset")</p>
