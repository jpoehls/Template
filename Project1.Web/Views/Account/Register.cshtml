@using Project1.Web.Controllers
@using Project1.Web.Models

@{
    ViewBag.Title = "Register";
}

<h2>@(ViewBag.Title)</h2>
<p>
    Use the form below to create a new account. 
</p>

@using(var form = Html.FormForAction((AccountController ctlr, RegisterModel model) => ctlr.Register(model)))
{
    form.Render();
}