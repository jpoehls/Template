@using Project1.Web.Controllers
@using Project1.Web.Models

@{
    ViewBag.Title = "Change Password";
}

<h2>@(ViewBag.Title)</h2>
<p>
    Use the form below to change your password. 
</p>

@using (var form = Html.FormForAction((AccountController ctlr, ChangePasswordModel b) => ctlr.ChangePassword(b)))
{
    form.DisplayName = "Change Password";
    form.Render();
}
